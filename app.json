{
  "android" : {
    "permissions" : [
      "CAMERA",
      "READ_EXTERNAL_STORAGE",
      "WRITE_EXTERNAL_STORAGE",
      "WRITE_SETTINGS"]
  },
  "expo": {
    "name": "kinujo",
    "slug": "kinujo",
    "version": "1.0.0",
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.example.kinujo",
      "googleServicesFile": "./google-services"
    },
    "orientation": "portrait",
    "icon": "./app/assets/Images/icon.png",
    "splash": {
      "image": "./app/assets/Images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "permissions" : [
      "CAMERA",
      "READ_EXTERNAL_STORAGE",
      "WRITE_EXTERNAL_STORAGE",
      "WRITE_SETTINGS"],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.kinujo"
    },
    "web": {
      "favicon": "./app/assets/Images/favicon.png"
    },
    "description": "",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
