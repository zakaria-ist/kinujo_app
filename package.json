{
  "scripts": {
    "start": "react-native start --reset-cache",
    "swm": "watchman watch-del-all && react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "fixbuild": "node ./android-release-gradle-fix.js;",
    "postinstall": "npx patch-package; yarn fixbuild; npx jetify; cd ios; pod install",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "ra": "yarn kj; fastlane android rl; yarn kj; yarn noti; yarn s",
    "da": "yarn kj; fastlane android db; yarn kj; yarn noti; yarn s",
    "o": "open android/app/build/outputs/apk",
    "s": "serve android/app/build/outputs",
    "kj": "killall -9 java",
    "noti": "osascript -e 'display notification \"Đã build xong!!!!\" with title \"Success!\"'"
  },
